{
  "manifest_version": 3,
  "name": "Gemini Chat Index",
  "version": "0.0.1",
  "description": "Adds a quick index to Gemini chat history.",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://gemini.google.com/*"],
      "css": [
        "vendor/tippy.css",
        "styles.css"
      ],
      "js": [
        "vendor/popper.min.js",
        "vendor/tippy-bundle.umd.min.js",
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "images/icon.svg",
    "48": "images/icon.svg",
    "128": "images/icon.svg"
  }
}
